   "source": [
    "l=int(input(\"Enter the value of lambda: \"))\n",
    "x=int(input(\"Enter the value of x: \"))\n",
    "e=2.718281\n",
    "fact=1\n",
    "for i in range(1,x+1):\n",
    "    fact*=i\n",
    "p=((l**x)*(e**(-l)))/fact\n",
    "print(\"The probability is : {:.4f} \".format(p))"
   ]
